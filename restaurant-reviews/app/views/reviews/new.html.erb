<%= form_for [ @restaurant, @review ] do |f| %>
  <div class="field">
    <%= f.label :rating %> <br />
    <%= f.text_field :rating %>
  </div>

  <div class="field">
    <%= f.label :body %><br />
    <%= f.text_field :body %>
  </div>
  <div class="actions">
    <%= f.submit "Add a Review"%>
  </div>
<% end %>



<p>
<%= link_to 'Home', root_path %>
</p>
